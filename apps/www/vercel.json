{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm install --no-frozen-lockfile && cd apps/www && pnpm run build:registry && next build",
  "outputDirectory": "apps/www/.next",
  "installCommand": "cd ../.. && pnpm install --no-frozen-lockfile",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- .",
  "functions": {
    "app/**": {
      "maxDuration": 30
    }
  },
  "env": {
    "SKIP_ENV_VALIDATION": "1"
  }
}
