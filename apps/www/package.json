  "private": true,
  "scripts": {
    "build": "contentlayer2 build && pnpm build:registry && next build",
    "build:registry": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.mts && prettier --loglevel silent --write \"registry/**/*.{ts,tsx,mdx}\" --cache",
    "dev": "concurrently \"contentlayer2 dev\" \"next dev --port 3333\"",
    "start": "next start --port 3333",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "type-check": "tsc --noEmit",
    "build:standalone": "pnpm install --no-frozen-lockfile && pnpm --filter=shadcn build && pnpm run build"
  },
